let randomNum = Math.floor(Math.random() * 10);  // هيطلع رقم عشوائي بين 0 و 9


let userGusse = Inputevents.getnumber("Enter a number between 0 and 9: ") // هيطلب من المستخدم يدخل رقم بين 0 و 9

if (userGussse == randomNum) {
    console.log("You win!");
}
else {
    console.log(`You lose! The correct number is ${randomNum}
        Try again!😶‍🌫️🙂`);
}


